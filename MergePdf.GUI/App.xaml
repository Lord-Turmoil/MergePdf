<prism:PrismApplication x:Class="MergePdf.GUI.App"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:prism="http://prismlibrary.com/">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Views/Styles/BaseStyle.xaml" />
                <ResourceDictionary Source="Views/Styles/MenuTabStyle.xaml" />
                <ResourceDictionary Source="Views/Styles/FileListStyle.xaml" />
                <ResourceDictionary Source="Views/Styles/FormStyle.xaml" />
                <ResourceDictionary Source="Views/Styles/IndexStyle.xaml" />

                <ResourceDictionary Source="Views/Templates/MenuTabTemplate.xaml" />
                <ResourceDictionary Source="Views/Templates/FileListTemplate.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</prism:PrismApplication>